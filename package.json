{
  "name": "rhiz-telegram-bot",
  "version": "1.0.0",
  "description": "AI-powered relationship intelligence bot for Telegram",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:setup": "echo 'Run database/schema.sql in your Supabase project'"
  },
  "dependencies": {
    "telegraf": "^4.15.6",
    "openai": "^4.26.0",
    "elevenlabs": "^0.8.1",
    "@supabase/supabase-js": "^2.39.3",
    "googleapis": "^131.0.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "fluent-ffmpeg": "^2.1.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/cors": "^2.8.17",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "prettier": "^3.1.1"
  },
  "keywords": ["telegram", "bot", "ai", "relationships", "voice"],
  "author": "Israel Wilson",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
