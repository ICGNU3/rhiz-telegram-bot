{
  "name": "rhiz-telegram-bot",
  "version": "1.0.0",
  "description": "AI-powered relationship intelligence bot for Telegram",
  "main": "dist/index.js",
  "scripts": {
    "start": "node test-railway.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "postbuild": "echo 'Build completed successfully'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/.*\\.test\\.ts",
    "test:integration": "jest --testPathPattern=tests/.*\\.test\\.ts --testNamePattern='Voice Processing Pipeline'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:setup": "echo 'Run database/schema.sql in your Supabase project'",
    "db:test": "node -e \"require('./src/db/supabase').testConnection().then(console.log)\"",
    "railway:build": "npm run build && echo 'Railway build completed'",
    "railway:start": "node railway-start.js"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.9.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elevenlabs": "^1.59.0",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^131.0.0",
    "helmet": "^7.1.0",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.26.0",
    "telegraf": "^4.15.6",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.6",
    "@types/node-telegram-bot-api": "^0.64.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "relationships",
    "voice"
  ],
  "author": "Israel Wilson",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
